{
  "name": "@gzaripov/talkback",
  "version": "2.1.0",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "types": "typings/index.d.ts",
  "license": "MIT",
  "description": "A node.js HTTP proxy that records and playbacks requests",
  "scripts": {
    "build": "npm run type-check && npm run build:cleanup && npm run build:rollup && npm run build:typings",
    "build:type-check": "tsc --noEmit",
    "build:cleanup": "rm -rf dist",
    "build:rollup": "rollup -c",
    "build:typings": "tsc --declaration --emitDeclarationOnly --outDir dist/typings",
    "type-check": "tsc --noEmit",
    "start": "node ./example/start.js",
    "test": "npm run type-check && jest",
    "jest": "jest",
    "rollup": "rollup",
    "release": "standard-version"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "async-exit-hook": "^2.0.1",
    "content-type": "^1.0.4",
    "fast-deep-equal": "^2.0.1",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.0",
    "@types/content-type": "^1.1.3",
    "@types/eslint": "^4.16.6",
    "@types/jest": "^24.0.11",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.0.3",
    "@types/node-fetch": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "del": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.7.1",
    "jest-environment-node": "^24.7.1",
    "nyc": "^13.3.0",
    "prettier": "^1.17.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-copy": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "standard-version": "^5.0.2",
    "typescript": "^3.4.5"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gzaripov/talkback"
  },
  "keywords": [
    "http-proxy",
    "record",
    "vcr",
    "playback"
  ],
  "author": "Grigory Zaripov"
}
